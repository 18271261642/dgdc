<?xml version="1.0" encoding="utf-8"?>
<resources>
    //MultipleStatusView
    <declare-styleable name="MultipleStatusView">
        <attr name="loadingView" format="reference" />
        <attr name="errorView" format="reference" />
        <attr name="emptyView" format="reference" />
        <attr name="noNetworkView" format="reference" />
        <attr name="contentView" format="reference" />
    </declare-styleable>

    <!--CircleImageView-->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>
    <!--TimerView-->
    <declare-styleable name="Timer">
        <attr name="text" format="reference|string" />
        <attr name="second" format="integer" />
    </declare-styleable>
    <declare-styleable name="ItemSportView">
        <attr name="sport_title_value" format="reference|string" />
        <attr name="sport_unitl_value" format="reference|string" />
        <attr name="sport_type_icon" format="reference" />
        <attr name="sport_title_color" format="color"></attr>
        <attr name="sport_value_color" format="color"></attr>
        <attr name="sport_unitl_color" format="color"></attr>
        <attr name="sport_title_size" format="dimension"></attr>
        <attr name="sport_unitl_size" format="dimension"></attr>
        <attr name="sport_value_size" format="dimension"></attr>
    </declare-styleable>

    <!--BikeItemView-->
    <declare-styleable name="ItemView">
        <attr name="itemLeftText" format="reference|string" />
        <attr name="itemLeftTextColor" format="color"></attr>
        <attr name="itemLeftTextSize" format="dimension"></attr>
        <attr name="itemRightText" format="reference|string" />
        <attr name="itemRightTextColor" format="color"></attr>
        <attr name="itemRightTextSize" format="dimension"></attr>
        <attr name="leftIcon" format="reference" />
        <attr name="rightIcon" format="reference" />
        <attr name="isShowLeftIcon" format="boolean" />
    </declare-styleable>
    <declare-styleable name="TitleView">
        <attr name="titleLeftIcon" format="reference" />
        <attr name="titleRightIcon" format="reference"></attr>
        <attr name="calenderIcon" format="reference"></attr>
        <attr name="titletextSize" format="dimension"></attr>
        <attr name="titletextvalue" format="reference|string" />
        <attr name="titletextColor" format="color"></attr>
    </declare-styleable>

    <!--pickerview-->
    <declare-styleable name="pickerview">
        <attr name="pickerview_gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="pickerview_textSize" format="dimension" />
        <attr name="pickerview_textColorOut" format="color" />
        <attr name="pickerview_textColorCenter" format="color" />
        <attr name="pickerview_dividerColor" format="color" />
    </declare-styleable>

    <declare-styleable name="DatePicker">
        <attr name="type">
            <flag name="all" value="0" />
            <flag name="year_month_day_hour_min" value="1" />
            <flag name="year_month_day" value="2" />
        </attr>
        <attr name="cyclic" format="boolean" />
        <attr name="showLabel" format="boolean" />
    </declare-styleable>
    <!--frienditem-->
    <declare-styleable name="FriendItemView">
        <attr name="friendItemviewValueStr" format="reference|string" />
        <attr name="friendItemviewTitleStr" format="reference|string" />
        <attr name="friendItemviewValueColor" format="color"></attr>
        <attr name="friendItemviewTitleColor" format="color"></attr>
        <attr name="friendItemviewValueSize" format="dimension"></attr>
        <attr name="friendItemviewTitleSize" format="dimension"></attr>
    </declare-styleable>
</resources>